  <section class="section">

    <div class="container">
        <% if(superhero !== null){ %>

            <div class="columns is-desktop">
              <div class="column is-one-quarter">
                <div class="card">
                  <div class="card-image">
                    <figure class="image is-1by1">
                      <% if(superhero.image.url !== null){%>
                        <img
                        src="<%= superhero.image.url %>"
                        alt="<%= superhero.name %>"
                        onerror="javascript:this.src='/img/img.png'"
                      /> <% } else{ %>  
                      <img
                        src="https://bulma.io/images/placeholders/128x128.png"
                        alt="<%= superhero.name %>"
                      />
                      <% } %>
                    </figure>
                  </div>
                  <div class="card-content">
                    <div class="media">
                      <div class="media-content">
                        <p class="title is-4"><%= superhero.name %></p>
                        <p class="subtitle is-6">
                          
                          <% if(superhero.biography.publisher  !== 'null'){%>
                            <%=superhero.biography.publisher %> <% } else{ %> - <% } %>
                        </p>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
              <div class="column">
                <div class="box">
                  <div class="column">
                    
                    <h1 class="title">
                      
                      <% if(superhero.biography["full-name"]  !== 'null'){%>
                        <%=superhero.biography["full-name"] %> <% } else{ %> - <% } %>
                    </h1>
    
                    <div class="columns is-centered is-vcentered">
                      <div class="column">
                        <h2 class="title is-5">
                          Alignment
                        </h2>
                        
                        <% if(superhero.biography.alignment !== 'null'){%>
                          <%=superhero.biography.alignment%> <% } else{ %> - <% } %>
                      </div>
                      <div class="column">
                        <h2 class="title is-5">
                          Race
                        </h2>
                        <% if(superhero.appearance.race !== 'null'){%>
                        <%=superhero.appearance.race%> <% } else{ %> - <% } %>
                      </div>
                      <div class="column">
                        <h2 class="title is-5">
                          Base
                        </h2>
                        
                        <% if(superhero.work.base !== 'null'){%>
                          <%=superhero.work.base%> <% } else{ %> - <% } %>
                      </div>
                    </div>
                    <div class="columns is-centered is-vcentered">
                      <div class="column">
                        <h2 class="title is-5">
                          Height
                        </h2>
                        
                        <% if(superhero.appearance.height[0] !== 'null'){%>
                          <%= superhero.appearance.height[0]%> <% } else{ %> - <% } %>
                      </div>
                      <div class="column">
                        <h2 class="title is-5">
                          Weight
                        </h2>
                        
                        <% if(superhero.appearance.weight[1] !== 'null'){%>
                          <%= superhero.appearance.weight[1]%> <% } else{ %> - <% } %>
                      </div>
                      <div class="column">
                        <h2 class="title is-5">
                          First Appearance
                        </h2>
                        
                        <% if(superhero.biography["first-appearance"] !== 'null'){%>
                          <%= superhero.biography["first-appearance"]%> <% } else{ %> - <% } %>
                      </div>
                    </div>
                    <div class="columns is-centered is-vcentered">
                      <div class="column">
                        <h2 class="title is-5">
                          Strength
                        </h2>
                        
                        <% if(superhero.powerstats.strength !== 'null'){%>
                          <%= superhero.powerstats.strength%> <% } else{ %> - <% } %>
                      </div>
                      <div class="column">
                        <h2 class="title is-5">
                          Intelligence
                        </h2>
                        
                        <% if(superhero.powerstats.intelligence !== 'null'){%>
                          <%= superhero.powerstats.intelligence%> <% } else{ %> - <% } %>
                      </div>
                      <div class="column">
                        <h2 class="title is-5">
                          Speed
                        </h2>
                        
                        <% if(superhero.powerstats.speed !== 'null'){%>
                          <%= superhero.powerstats.speed%> <% } else{ %> - <% } %>
                      </div>
                    </div>
                    <div class="columns is-centered is-vcentered">
                      <div class="column">
                        <h2 class="title is-5">
                          Aliases
                        </h2>
                        
                        <% if(superhero.biography.aliases !== 'null'){%>
                          <%= superhero.biography.aliases%> <% } else{ %> - <% } %>
                      </div>
                    </div>
                  </div>
    
                 
                </div>
              </div>
            </div>
    
                
            <% } %>

